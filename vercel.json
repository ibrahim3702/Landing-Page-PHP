{
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.7.4"
        }
    },
    "routes": [
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1"
        },
        {
            "src": "/about",
            "dest": "/api/about.php"
        },
        {
            "src": "/projects",
            "dest": "/api/projects.php"
        },
        {
            "src": "/resources",
            "dest": "/api/resources.php"
        },
        {
            "src": "/contact",
            "dest": "/api/contact.php"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}